    "enzyme": "^2.9.1",
    "html-webpack-plugin": "^2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "prop-types": "^15.5.10",
    "react-test-renderer": "^15.6.1",
